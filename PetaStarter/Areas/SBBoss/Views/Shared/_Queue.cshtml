@model IEnumerable<Speedbird.Queue>
<style>
    .alert {
        padding-bottom:0px;
    }
</style>

<br />
@if (Model.Count() > 0)
{
<div class="row alert alert-primary alert-dismissible fade show" role="alert" style="position:absolute; top:15px;left:80px; border:3px groove #ffffec">
    @foreach (var item in Model)
    {
        <a href="/SBBoss/SR/SRQueueIndex/?st=@item.ServiceTypeId">
            <div class="col-sm-auto">
                <div class="qItem alert alert-warning text-center">
                    <span class="h4">@((Speedbird.ServiceTypeEnum)@item.ServiceTypeId)  <span class="badge badge-danger">@item.Count</span> </span>
                </div>
            </div>
        </a>
    }
        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
</div>
}
<script>
    $(document).ready(function () {
        $('.alert').alert();
    });
</script>
