@model  Speedbird.ServiceRequestDets
<style>
    .table {
        width: 100%;
    }
</style>
@if (Model != null)
{
    <div class="alert alert-secondary">
    <table class="table table-sm table-dark">
        <tr>
            <th>
                <span class="h3">Booking No:      <span class="text-warning"> @Model.BookingNo</span></span>
            </th>
            <th>
                <span class="h3"><span class="text-muted">Booked on:</span> @Model.DT</span>
            </th>
            <th>
                <span class="h3"><span class="text-muted">Status:</span>  @((Speedbird.SRStatusEnum)Model.SRStatusID)</span>
            </th>
            <th>
                <span class="h3"><span class="text-muted">Emp. Name:</span>  @Model.UserName</span>
            </th>            
        </tr>
        
    </table>
</div>
}

